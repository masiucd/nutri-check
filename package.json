{
  "name": "nutri-check",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "biome": "biome",
    "db:generate": "drizzle-kit generate",
