    "@radix-ui/react-tooltip": "^1.1.6",
    "@react-router/node": "^7.1.0",
    "@react-router/serve": "^7.1.0",
    "argon2": "^0.41.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
