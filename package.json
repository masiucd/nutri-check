    "db:migrateV2": "cross-env-shell DB_MIGRATION=true tsx app/.server/db/migrate.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rimraf node_modules && pnpm install"
  },
  "dependencies": {
